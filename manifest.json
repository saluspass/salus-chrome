{
"manifest_version": 2,
    "name": "[your extension name here]",
    "description": "[your extension description here]",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}