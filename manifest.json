{
"manifest_version": 2,
    "name": "Salus",
    "description": "Chrome extension for autocomplete login forms",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}